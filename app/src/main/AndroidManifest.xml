<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sourcewords">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SourceWords">
        <activity
            android:name=".ui.login.view.LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.mine.view.AddPlanActivity"></activity>
        <activity android:name=".ui.mine.view.ChangePwdActivity"></activity>
        <activity android:name=".ui.mine.view.UserInfoActivity"></activity>
        <activity android:name=".ui.login.view.RegisterActivity"></activity>
    </application>

</manifest>